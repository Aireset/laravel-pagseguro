{
    "name": "zhiru/laravel-pagseguro",
    "description": "PagSeguro Checkout Transparente, Pagamento Recorrente (Assinaturas) e Split no Laravel 5.*",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "pagseguro", "transparente", "recorrente", "assinaturas"],
    "authors": [
        {
            "name": "Felipe Almeman",
            "email": "felipe@aireset.com.br"
        }
    ],
    "autoload": {
        "psr-4": {
            "Zhiru\\PagSeguro\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0",
        "illuminate/support": "^5.2|^6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zhiru\\PagSeguro\\PagSeguroServiceProvider"
            ],
            "aliases": {
                "PagSeguro": "Zhiru\\PagSeguro\\PagSeguroFacade",
                "PagSeguroSplit": "Zhiru\\PagSeguro\\PagSeguroSplitFacade",
                "PagSeguroRecorrente": "Zhiru\\PagSeguro\\PagSeguroRecorrenteFacade",
                "PagSeguroBoleto": "Zhiru\\PagSeguro\\PagSeguroBoletoFacade"
            }
        }
    }
}
